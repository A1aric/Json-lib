<?xml version="1.0" encoding="ISO-8859-1"?>
<document>
	<properties>
		<title>JsTester</title>
		<author email="aalmiray@users.sourceforge.net">
			Andres Almiray
		</author>
	</properties>
	<body>
		<release version="N/A" date="2006-08-15"
			desc="Current working version">
			<action dev="Andres Almiray" type="add">
            Refactored converter package into 
            EZMorph (http://ezmorph.sourceforge.net)
			</action>
			<action dev="Andres Almiray" type="fix" issue="1540196">
            JSONArray did not cerate multidimensional arrays correctly.
			</action>
			<action dev="Andres Almiray" type="fix" issue="1540137">
            JSONObject's fromObject() and fromBean() did not support
            DynaBeans properly
			</action>
			<action dev="Andres Almiray" type="fix" issue="1538330">
				XmlSerializer writes wrong JSONFunction data.
			</action>
			<action dev="Andres Almiray" type="fix" issue="1538095">
				JSONObject.fromObject() does not work properly when passed a
				boolean as parameter in JVM 1.5+
			</action>
		</release>
		<release version="0.7.1" date="2006-08-09"
			desc="Service release">
			<action dev="Andres Almiray" type="fix" issue="1537557">
				Json-lib had wrong class numbers for JDK 1.3.1
			</action>
		</release>
		<release version="0.7" date="2006-08-07"
			desc="Fourth public release">
			<action dev="Andres Almiray" type="fix" issue="1529946">
				Make Json-lib JDK 1.3.1 compatible
			</action>
		</release>
		<release version="0.6" date="2006-07-24"
			desc="Third public release">
			<action dev="Andres Almiray" type="add">
				Added support for DynaBean and java bean conversions.
			</action>
			<action dev="Andres Almiray" type="update">
				Updated the XML generated by XMLSerializer.
				<br />
				JSONArray uses &lt;a&gt; for root element, JSONObject
				uses &lt;o&gt; for root element.
			</action>
			<action dev="Andres Almiray" type="update">
				Updated usage documentation.
			</action>
		</release>
		<release version="0.4" date="2006-07-11"
			desc="Second public release">
			<action dev="Andres Almiray" type="add">
				Added test suites for each package and for full project.
			</action>
			<action dev="Andres Almiray" type="fix" issue="1516306">
				Support js functions as type.
			</action>
			<action dev="Andres Almiray" type="update">
				Moved common methods of JSONObject, JSONArray to
				JSONUtils.
			</action>
			<action dev="Andres Almiray" type="update">
				Renamed XML to XMLSerializer. Now uses XOM to parse
				documents.
			</action>
			<action dev="Andres Almiray" type="update">
				Moved common assertion methods to Assertions (tests).
			</action>
			<action dev="Andres Almiray" type="delete">
				Deleted HTTP related classes.
			</action>
		</release>

		<release version="0.2" date="2006-07-01"
			desc="First public release">
			<action dev="Andres Almiray" type="add">
				First release.
			</action>
		</release>
	</body>
</document>
